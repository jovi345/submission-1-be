{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "function-paren-newline": "off",
    "comma-dangle": "off",
    "implicit-arrow-linebreak": "off",
    "no-shadow": "off",
    "no-console": "off"
  }
}
